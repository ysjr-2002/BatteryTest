<UserControl
    x:Class="BITools.UIControls.HZTabControlWithCollapse"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:BITools.UIControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <Grid>
        <TabControl
            x:Name="RightContainer"
            ItemContainerStyle="{StaticResource TabItem.BigData.Sub.Style}"
            SelectionChanged="RightContainer_SelectionChanged"
            Style="{StaticResource TabControl.Collapse.Style}" />
    </Grid>
</UserControl>
